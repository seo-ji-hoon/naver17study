package day1223;

public class Book317 {
	
	//Tire 클래스
	public class Tire {
		//메소드 선언
		public void roll() {
			System.out.println("회전합니다.");
		}
	}
	
	//HankookTire 클래스 
	public class HankookTire extends Tire {
		
		//메소드 재정의 (오버라이딩)
		
		@Override
		public void roll() {
			// TODO Auto-generated method stub
			System.out.println("한국 타이어가 회전합니다.");
		}	
	}

	//KumhoTire 클래스
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
