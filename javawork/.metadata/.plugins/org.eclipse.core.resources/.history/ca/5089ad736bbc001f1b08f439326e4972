		for(int i=2; i<=9; i++) {
			
			System.out.print(i+"단\t");
		}
		System.out.println();
		System.out.println("=".repeat(65));
		
		for(int n=2; n<=9; n++) {
			
			System.out.println(n);
			
			for(int nn=1;nn<=9; nn++) {
				
			}
		}
		
		
		
		
	}

}
