package jihoon_test;

public class Day1218ArrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char [] ch;
		
		ch=new char[4];
		
		ch[0]='a';
		ch[1]='b';
		ch[2]='c';
		ch[3]='d';
		
		System.out.println(ch);
		System.out.println("배열의 크기는?"+ch.length);
		
		for (int i=0;i<ch.length; i++) {
			
			System.out.println(ch));
			
		}
		
	}

}
