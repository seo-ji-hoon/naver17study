package day1217;

public class Ex6ArrayRank {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//등수 구하기- 동점은 동순위 부여
		int []score= {56,89,100,89,67};
		
		int[]rank=new int[5];
		
		for(int i=0;i<score.length;i++) {
			
			rank[i]=1;
			
			for(int j=0;j<score.length; j++) {
				
				if(score[1]<score[j]) {
					rank[i]++;
				}
			}
			
		}
		
		
	}

}
