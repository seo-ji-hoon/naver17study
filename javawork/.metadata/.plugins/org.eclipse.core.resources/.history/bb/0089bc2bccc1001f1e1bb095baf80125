package day1224;

import java.util.List;
import java.util.Vector;

public class Ex12FileList {
	
	static final String FLIENAME="c:/naver1210/sawon.txt";
	
	List<String> sawonList=new Vector<String>();
	
	public Ex12FileList() {
		
		//파일에서 이름을 읽어서 sawonList에 추가해주세요
		
		
		
		
	}
	
	public void sawonMemberList() {
		
		//사원 이름을 번호와 함께 출력해주세요.
		
		System.out.println("list2 의데이타 갯수"+sawonList.size()+",할당갯수 : "
				+((Vector<String>)sawonList).capacity());//capacity() 메서드는 오버라이드 메서드가 아님
		
				System.out.println("출력방법 2");
				
				for(int i=0; i<sawonList.size();i++) {
					System.out.print(i+1+":"+sawonList.get(i)+" ");
				}
				System.out.println();
		
		
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//파일에서 이름을 읽어서 sawonList에 추가해주세요.
		Ex12FileList ex12=new Ex12FileList();
		ex12.sawonMemberList();
		
		
	}
	


}
