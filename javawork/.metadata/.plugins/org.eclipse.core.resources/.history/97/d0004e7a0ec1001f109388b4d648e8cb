package jihoon_test;

//Animal (부모)클래스

class Animal {
	
	void sound() {
		System.out.println("Animal makes a sound");
	}
	
}

class Dog extends Animal {
	void sound () {
		System.out.println("Dog barks");
	}
}


public class AnimalTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
