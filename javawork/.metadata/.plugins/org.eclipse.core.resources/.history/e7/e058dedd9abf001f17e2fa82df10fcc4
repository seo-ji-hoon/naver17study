package jihoon_test;

import java.util.Scanner;

public class JihoonTest1220 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
	
		for (int i = 2; i <= 9; i++) {
		    // 안쪽 for문: 각 단에서 곱해지는 수를 나타내는 변수 j를 1부터 9까지 반복
		    for (int j = 1; j <= 9; j++) {
		        // 구구단의 식을 출력
		        System.out.println(i + " * " + j + " = " + (i * j));
		    }
		}
		출처: https://creativevista.tistory.com/entry/Java자바-for-문-초보자를-위한-완벽한-안내서 [YJ Dev👩‍💻코딩 초보를 위한 학습 블로그:티스토리]
		
		
		
		
		
	}
}

