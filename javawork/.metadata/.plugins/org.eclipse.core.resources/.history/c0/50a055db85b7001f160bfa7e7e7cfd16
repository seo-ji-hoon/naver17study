package day1211;

public class Ex7DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="candy";
		int age=32;
		float height=163.8f;
		double weight=56.7;
		char blood='B';
		/*
		 * 이름 : Candy님
		 * 나이 : 32세
		 * 키 : 163.8cm
		 * 몸무게 : 56.7kg
		 * 혈액형 : B형
		 * 
		 * printf를 이용해서 위와같이 출력해보세요
		 */
		
		System.out.printf("이름: %s\n나이:%s세\n키:%s\n%s몸무게:\n혈액형:%s형",name,age,height,weight,blood);
	}

}
