package bookjava;

public class Ex1If {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 숫자형 int 정수 score에 93을 입력
		int score=90;
		
		if(score>=0 || score<=100) {
			System.out.println("잘못된점수입니다.");
		}else if(score>=90) {
			System.out.println("90점보다 크네요");
			System.out.println("A등급입니다.");
		}
	}

}
