package day1216;

import java.util.Scanner;

public class Ex10Gugudan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		/*
		 * 
		 * 구구단 한페이지에 나오게
		 * -----------------------줄 그어주고
		 * 2x1=2  3x1=3 - - - - - -
		 * 
		 * 다중 for문
		 */
		
		int n,total=0;
		
		for(int i=2; i<=9; i++) {
			
			System.out.print(i+"단"+"     ");
		}
		System.out.println();
		System.out.println("=".repeat(65));
		System.out.println();
		
		for()
	}
	
}
