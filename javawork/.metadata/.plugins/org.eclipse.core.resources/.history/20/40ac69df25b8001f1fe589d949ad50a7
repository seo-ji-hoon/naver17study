package day1212;

public class Ex1Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//증감 연산자, ++변수 (전치:1순위)앞에붙는역활, 변수++(후치:끝순위)
		int a,b,m,n;
		a=b=5;
		m=++a; //먼저 a가 증가된후 m으로 대입된다.
		n=b++; //먼저 대입후 b가 증가된다.
		
		System.out.printf("a=%d,b=%d,m=%d,n=%d\n",a,b,m,n);
		
		a=b=m=n=5;
		m=a++*++b;//5*6=30 m은 30으로 출력
		System.out.printf("a=%d,b=%d,m=%d,n=%d\n",a,b,m,n);
		
		n=a++ + ++b;
		
	}

}
