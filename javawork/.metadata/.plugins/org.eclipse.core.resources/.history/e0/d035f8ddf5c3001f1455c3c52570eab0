package day1227;

/*
 * 
 * 자바에서의 함수형 프로그램인 람다(Lambda) 표현식은 인터페이스를
 * 사용하는 익명 내부 클래스의 또다른 표현식이다.
 * 단, 인터페이스는 단 하나의 추상 메서드만 가지고 있어야한다.
 * 
 * 람다는 ^ 그리스어 아파벳의 11번째 글자이다.
 */


@FunctionalInterface //메서드가 하나인지 아닌지 검증하는 역활, 두개일 경우 오류발생
interface Orange {
	
	public void write();// 추상 메서드
//	public void study(); <메서드를 두개 추가시 오류발생>
			
}

@FunctionalInterface
interface DataAdd {
	
	public void add(int x, int y);
		
}

public class Ex2Lambda {
	
	//우리가 알고있는 익명 내부 클래스 형태로 Orange 를 구현해보자.
	Orange or=new Orange() {
		
		@Override
		public void write() {
			// TODO Auto-generated method stub
			
		}
	};
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
	}

}
