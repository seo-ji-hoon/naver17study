package jihoon_test;

public class Day1214 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		int number = 10;
		
		
		number += 5;	
		// number 에 값은 몇일까요? 15
			
	
		number -= 1;
		// number의 값은? 9
		
		
		
		number = 0;
		number+=2;
		
		// number 의 값은? 12
		
	
		
	}

}
