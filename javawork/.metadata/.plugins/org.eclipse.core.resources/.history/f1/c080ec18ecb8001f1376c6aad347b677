package day1213;

import java.util.Scanner;

public class Ex2Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		char ch;
		
		switch(ch ) 
		{
		case 'a':
		case 'A':
			System.out.println("Apple");
			break;
		
		}
		
		
	}

}
