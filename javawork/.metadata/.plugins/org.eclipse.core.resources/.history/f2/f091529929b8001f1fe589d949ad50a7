package day1212;

public class Ex2Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//산술연산자 +,-,*,/,%
		
		int su1=10;
		int su2=4;
		
		System.out.println(su1+su2);
		System.out.println(su1-su2);
		System.out.println(su1*su2);
		System.out.println(su1/su2); //몫
		System.out.println(su1%su2); //나머지
		
		
		
		
		
	}

}
