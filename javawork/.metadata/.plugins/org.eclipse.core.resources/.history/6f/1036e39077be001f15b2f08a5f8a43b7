package day1220;

//부모
class SuperObj2{
	
	private String schoolName;
	
	public SuperObj2() {
		
		schoolName="청담고등학교";
	}
	
	SuperObj2(String schoolName){
		
		this.schoolName=schoolName;
		
	}
	
	public void write() {
		
		System.out.println("학교명 : "+schoolName);
	}
}

//상속
class SubObj2 extends SuperObj2 {
	
	
}
