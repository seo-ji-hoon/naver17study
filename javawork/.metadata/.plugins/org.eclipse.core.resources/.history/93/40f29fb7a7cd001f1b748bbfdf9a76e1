package bookjava;

public class Ex1If {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 숫자형 int 정수 score에 93을 입력
		int score=80;
		
		if(score>=90) {
			System.out.println("90점보다 큽니다.");
			System.out.println("A등급이네요.");
			
		}else if(score<=80){
			System.out.println("90점보다 작네요");
			System.out.println("B등급입니다.");
		}
	}

}
