package day1212;

public class book94 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int charCode='A';
			
		if((65<=charCode)&(charCode<=90))//char코드가 65미만 90이상일떄
		{
			System.out.println("대문자이군요");
		}
		if((97<=charCode)&(charCode<=122))//char코드가 65미만 90이상일떄
		{
			System.out.println("소문자이군요");
		}
		if((48<=charCode)&(charCode<=57))//char코드가 65미만 90이상일떄
		{
			System.out.println("0~9 숫자이군요");
		}
	}

}
