package day1217;

public class Ex4ArrayString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//#1 String 배열
		
		String[] str1;
		str1=new String[4]; //객체인 경우는 null 값으로 초기값 지정이됨
		str1[0]="Hello";
		str1[3]="Kitty";
		
		for(int i=0;i<str1.length; i++) {
			
			System.out.println("str1["+i+"]="+str1[i]);
		}
		
		
		
		
		
		
		
		
		
		
		
	}

}
