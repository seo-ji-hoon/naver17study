package day1213;

public class Ex1Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//switchë¬¸
		int n=1;
		
		switch(n) {
		case 1:
			System.out.println("one");
		case 2:
			System.out.println("two");
		case 3:
			System.out.println("three");
		default:
			System.out.println("other number");
		}
	}

}
